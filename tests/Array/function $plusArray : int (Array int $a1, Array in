function $plusArray : int (Array int $a1, Array int $a2, int $size) {
	int $i;
	Array int $sum;

	$sum : new Array int;
	for ($i : 0; $i < $size; $i+:1) {
		$sum[$i] : $a1[$i] + $a2[$i];
	}
}

function $main : void ()
{
	Array int $c;
	Array int $d;
	Array int $result;
	int $j;

	$printstring("Func w array input, return array");
	$printstring("Should print -3 8");

	$c : new Array int;
	$d : new Array int;
	$c[0] : 1;
	$d[0] : -3;
	$c[1] : 2;
	$d[1] : 4;

	$result : $plusArray($c,$d, 2);	
	$printint( $result[0] );		
	$printint( $result[1] );		
}
